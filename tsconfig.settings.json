{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir":"./build",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "composite": true,
    "alwaysStrict": true
    // "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "build",
    "es"
  ]
}